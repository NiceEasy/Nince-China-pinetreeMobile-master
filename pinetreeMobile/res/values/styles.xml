<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    
    
    
    <style name="TransparentTheme" parent="@android:style/Theme.Holo.Light.NoActionBar">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="toolbar_body">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">50dip</item>
        <item name="android:background">@drawable/toolbar_background</item>
    </style>

    <style name="title_body">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">50dip</item>
        <item name="android:background">@drawable/title_background</item>
    </style>

    <style name="title_menu_button">
        <item name="android:layout_margin">5dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/title_button</item>
        <item name="android:minWidth">55dip</item>
        <item name="android:minHeight">35dip</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>

    <style name="title_button">
        <item name="android:layout_margin">5dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/title_button</item>
        <item name="android:minWidth">55dip</item>
        <item name="android:minHeight">35dip</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>

	<style name="title_button_recommended">
        <item name="android:layout_margin">5dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/title_button_recommended</item>
        <item name="android:minWidth">55dip</item>
        <item name="android:minHeight">35dip</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>

    <style name="title_back_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minWidth">70dip</item>
        <item name="android:minHeight">35dip</item>
        <item name="android:layout_margin">5dip</item>
        <item name="android:background">@drawable/title_back_button</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>

    <style name="title_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="switcher">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textOn"></item>
        <item name="android:textOff"></item>
    </style>

    <style name="list">
        <item name="android:listSelector">@drawable/list_normal_background_selector</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:cacheColorHint">#00000000</item>
        <item name="android:background">@null</item>
    </style>

    <style name="iosDialog" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/alert_dialog_background</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>

    <style name="iosWaitingDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
    </style>

    <style name="windowAnimation" parent="android:Animation">
        <item name="@android:windowEnterAnimation">@anim/fade_in</item>
        <item name="@android:windowExitAnimation">@anim/fade_out</item>
    </style>

    <style name="popupAnimation" parent="android:Animation" mce_bogus="1">
        <item name="android:windowEnterAnimation">@anim/pop_enter</item>
        <item name="android:windowExitAnimation">@anim/pop_exit</item>
    </style>
    <style name="actionbar_def_style" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">@color/transparent</item>
        <item name="android:displayOptions">useLogo</item>
    </style>

    <style name="IFTheme" parent="android:Theme.Holo.Light">
        <item name="android:actionBarStyle">@style/actionbar_def_style</item>
    </style>
    <style name="radiobutton_bottom">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:button">@null</item>
        <item name="android:layout_margin">1dip</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@drawable/radiobutton_text_selector</item>
        <item name="android:textSize">11sp</item>
    </style>


    <style name="FRActionBarDark" parent="@android:style/Widget.Holo.ActionBar">
        //<item name="android:background">@drawable/action_head_light</item>
    </style>
    
